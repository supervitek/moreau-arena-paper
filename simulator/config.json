    "stat_names": [
      "hp",
      "atk",
      "spd",
      "wil"
    ],
    "formulas": {
      "max_hp": "50 + 10 * hp",
      "base_dmg": "floor(2 + 0.85 * atk)",
      "movement": "1 if spd<=3, 2 if spd<=6, 3 if spd>=7",
      "dodge": "max(0, min(0.30, 0.025 * (spd - 1)))",
      "ability_range": "min(4, ceil(wil / 2))",
      "resist": "max(0, min(0.35, 0.03 * (wil - 1)))",
      "ability_power": "1.0 + 0.05 * wil"
    },
    "size_thresholds": {
      "1x1": "hp+atk <= 10",
      "2x1": "hp+atk <= 12",
      "2x2": "hp+atk <= 17",
      "3x2": "hp+atk > 17"
    }
  },
